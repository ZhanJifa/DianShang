        <script src="~/js/adminlte.min.js"></script>
        <script src="~/js/demo.js"></script>
        <script>
            $(function () {
                $("#ulMenu li a").click(function () {
                    //获取当前点击到的a标签 attr一个参数获取值
                    var url = $(this).attr('url');
                    //降获取到的路赋值 attr设置值
                    $("#iframes").attr("src", url);
                    //获取指定文本信息
                    var text = $(this).find('span').text();
                    if (url != undefined) {
                        text = $(this).text();
                    }
                    $("#breadcrumb").html("<a href='index.html' title='Go to Home' class='tip-bottom'><i class='icon-home'></i > 首页 > <a href = 'index.html' title = '" + text + "' class= 'tip-bottom' >" + text + "</a ></a >");
                    //添加一个样式 所有的兄弟标签移出样式
                    $(this).addClass('active').siblings().removeClass('active');
                })
            })
        </script>


